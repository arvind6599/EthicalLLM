    "def generate_prompt_to_SFT(harmful_prompt: str):\n",
    "    \"\"\"wrapper around the original harmful prompt to the SFT model\n",
    "    to ask the SFT model to generate the response in given JSON format.\n",
    "    \"\"\"\n",
    "    return f\"\"\"\n",
    "    Answer the following question:\n",
    "    {harmful_prompt}\n",
    "    Your response must be purely in JSON format, including:\n",
    "    {{\n",
    "    \"answer\": your answer to the question,\n",
    "    \"motivation\": your motivation behind the answer,\n",
    "    \"consequence\": the utility of the answer towards prompting yourself as a virtuous agent\n",
    "    }}\n",
    "    \"\"\"\n",
    "\n",
    "\n",
    "import random\n",
    "harmful_prompts_exp = random.choices(harmful_prompts, k=3)\n",
    "for harmful_prompt in harmful_prompts_exp:\n",
    "    input_to_SFT = generate_prompt_to_SFT(harmful_prompt)\n",
    "    print(input_to_SFT)\n",
    "    SFT_model_response = retrieve_pipe_response(pipe_SL, input_to_SFT, max_new_tokens=256)\n",
    "    print(\"Agent answer below:\\n\")\n",
    "    print(SFT_model_response[0])\n",
    "    print(SFT_model_response[1])\n",
    "    print(\"-----\\n\\n\")"
   ]
